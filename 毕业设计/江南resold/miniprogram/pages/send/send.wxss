.fix{
  position: fixed;
  right: 0rpx;
  bottom: 200rpx;
  animation: run 2s linear infinite;
  z-index: 1000; 
}
.fix,image{
  height: 145rpx;
  width: 120rpx;
}
@keyframes run {
  0%{
      bottom: 200rpx;
       width: 120rpx;
  }
  20%{
      bottom: 210rpx;
  width: 124rpx;

  }
  40%{
      bottom: 230rpx;
  width: 128rpx;

  }
  60%{
      bottom: 250rpx;
  width: 128rpx;

  }
  80%{
    bottom: 220rpx;
    width: 125rpx;
  }
  100%{
      bottom: 200rpx;
  width: 120rpx;
  }
}